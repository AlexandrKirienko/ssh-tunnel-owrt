#!/bin/sh /etc/rc.common

START=99
STOP=10

USE_PROCD=1
PROG="/root/ssh_tunnel.sh"

start_service() {
    procd_open_instance
    procd_set_param command "$PROG" start
    procd_set_param respawn 3600 10 500
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_close_instance
}

stop_service() {
    "$PROG" stop
}

restart() {
    stop
    sleep 2
    start
}
